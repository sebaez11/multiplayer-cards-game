<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>com.is.controllers.users.SignupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.is.controllers.users.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateRoom</servlet-name>
        <servlet-class>com.is.controllers.game_room.CreateRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JoinRoom</servlet-name>
        <servlet-class>com.is.controllers.game_room.JoinRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomList</servlet-name>
        <servlet-class>com.is.controllers.game_room.RoomListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateRoom</servlet-name>
        <url-pattern>/CreateRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JoinRoom</servlet-name>
        <url-pattern>/JoinRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomList</servlet-name>
        <url-pattern>/RoomList</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
